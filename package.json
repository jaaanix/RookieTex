{
  "name": "ai-pack",
  "main": "./lib/ai-pack",
  "version": "0.0.0",
  "description": "Helper for writing LaTeX and generating LaTeX projects",
  "keywords": [],
  "activationCommands": {
    "atom-workspace": [
      "ai-pack:convertListToTex",
      "ai-pack:addSection",
      "ai-pack:addSubSection",
      "ai-pack:addTable",
      "ai-pack:addUnorderedList",
      "ai-pack:addToc",
      "ai-pack:startTexDocument",
      "ai-pack:addImage",
      "ai-pack:makeBold",
      "ai-pack:makeItalic",
      "ai-pack:makeSmallCapitals",
      "ai-pack:makeTypewriter",
      "ai-pack:makeUnderlined",
      "ai-pack:addMathInlineMode",
      "ai-pack:addMathDisplayMode",
      "ai-pack:addPackage",
      "ai-pack:addNewPage",
      "ai-pack:addGlossaryEntry",
      "ai-pack:initializeTexProject",
      "ai-pack:initializeGlossary",
      "ai-pack:addDoubleQuotes",
      "ai-pack:addChapter",
      "ai-pack:addUrlLink",
      "ai-pack:addBegin",
      "ai-pack:addOrderedList",
      "ai-pack:addBackslash",
      "ai-pack:addCodeSnippet",
      "ai-pack:addFigure",
      "ai-pack:addMinipage"
    ]
  },
  "repository": "https://github.com/atom/ai-pack",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "mkdirp": "^0.5.1",
    "fs": "^0.0.2"
  },
  "devDependencies": {
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-standard": "^1.3.1"
  }
}
